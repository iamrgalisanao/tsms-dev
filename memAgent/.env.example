# TSMS Cipher Memory Agent Environment Configuration
# 
# CRITICAL: This file contains sensitive API keys.
# DO NOT commit this file to version control!
#
# Copy from .env.example and configure with your actual values

# OpenAI Configuration (Primary - Required)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_ORG_ID=your_org_id_here

# Alternative AI Providers (Optional)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
AZURE_OPENAI_API_KEY=your_azure_key_here
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_API_VERSION=2024-02-15-preview

# Cipher Configuration
CIPHER_LOG_LEVEL=info
CIPHER_PORT=3333
CIPHER_HOST=localhost

# MCP Configuration
MCP_SERVER_NAME=tsms-cipher-memory
MCP_SERVER_VERSION=1.0.0

# TSMS Project Specific Settings
TSMS_ENVIRONMENT=development
TSMS_MEMORY_MODE=comprehensive

# Performance Settings
CIPHER_MAX_CONCURRENT_REQUESTS=5
CIPHER_CACHE_TTL=3600
CIPHER_EMBEDDING_BATCH_SIZE=50

# Security Settings
CIPHER_API_RATE_LIMIT=100
CIPHER_ENABLE_CORS=true
CIPHER_ALLOWED_ORIGINS=http://localhost:*,https://localhost:*
